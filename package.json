{
  "name": "lucide-qwik",
  "version": "0.485.0",
  "description": "Lucide icon library package for Qwik applications",
  "author": {
    "name": "Luminescent",
    "url": "https://luminescent.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LuminescentDev/lucide-qwik.git"
  },
  "license": "LICENSE",
  "keywords": [
    "icon",
    "icons",
    "lucide",
    "qwik"
  ],
  "main": "./lib/index.qwik.mjs",
  "qwik": "./lib/index.qwik.mjs",
  "types": "./lib-types/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.qwik.mjs",
      "require": "./lib/index.qwik.cjs",
      "types": "./lib-types/index.d.ts"
    }
  },
  "files": [
    "lib",
    "lib-types"
  ],
  "engines": {
    "node": ">=15.0.0"
  },
  "private": false,
  "type": "module",
  "scripts": {
    "build": "pnpm build.icons && qwik build",
    "build.icons": "node --trace-warnings build.js",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "lint": "eslint \"src/**/*.ts*\"",
    "release": "tsx tools/release.ts",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "test": "echo no tests for now"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.12.1",
    "@types/eslint": "8.56.12",
    "@types/node": "^22.13.16",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "@typescript-eslint/parser": "8.29.0",
    "camelcase": "^8.0.0",
    "eslint": "8.57.1",
    "eslint-plugin-qwik": "^1.12.1",
    "lucide": "^0.486.0",
    "nx": "^20.7.0",
    "typescript": "5.8.2",
    "undici": "7.6.0",
    "vite": "6.2.4",
    "yargs": "^17.7.2"
  }
}
